<odoo>
    <!-- Sites -->
    <record id="demo_content_site_1" model="commown_translation_manager.content_site">
        <field name="name">Demo site 1</field>
        <field name="url">https://demo_1.commown.coop</field>
        <field 
            name="supported_langs"
            eval="[[ref('base.fr')], [ref('base.de')]]"
        />
    </record>

    <record id="demo_content_site_2" model="commown_translation_manager.content_site">
        <field name="name">Demo site 2</field>
        <field name="url">https://demo_2.commown.coop</field>
        <field 
            name="supported_langs"
            eval="[[ref('base.fr')], [ref('base.de')]]"
        />
    </record>

    <!-- Contenus (Site 1) -->
    <record id="demo_content_1" model="commown_translation_manager.content">
        <field name="name">Demo content 1</field>
        <field name="site_id" ref="demo_content_site_1"/>
        <field name="url_path">/1</field>
    </record>

    <record id="demo_content_2" model="commown_translation_manager.content">
        <field name="name">Demo content 2</field>
        <field name="site_id" ref="demo_content_site_1"/>
        <field name="url_path">/2</field>
    </record>

    <!-- Contenus (Site 2) -->
    <record id="demo_content_3" model="commown_translation_manager.content">
        <field name="name">Demo content 3</field>
        <field name="site_id" ref="demo_content_site_2"/>
        <field name="url_path">/3</field>
    </record>

    <!-- Versions (Site 1) -->
    <record id="demo_translation_1_fr" model="commown_translation_manager.version">
        <field name="content_path"/>
        <field name="content_id" ref="demo_content_1"/>
        <field name="url_path">/1/fr</field>
        <field name="language" eval="23"/>
        <field name="modification_date" eval="str(time.localtime(time.time())[0]+1)+'-01-01'"/>
    </record>

    <record id="demo_translation_1_de" model="commown_translation_manager.version">
        <field name="content_path"/>
        <field name="content_id" ref="demo_content_1"/>
        <field name="url_path">/1/de</field>
        <field name="language" eval="26"/>
        <field name="modification_date" eval="str(time.localtime(time.time())[0]+1)+'-01-01'"/>
    </record>

    <record id="demo_translation_2_fr" model="commown_translation_manager.version">
        <field name="content_path"/>
        <field name="content_id" ref="demo_content_2"/>
        <field name="url_path">/2/fr</field>
        <field name="language" eval="23"/>
        <field name="modification_date" eval="str(time.localtime(time.time())[0]+1)+'-01-01'"/>
    </record>

    <record id="demo_translation_2_de" model="commown_translation_manager.version">
        <field name="content_path"/>
        <field name="content_id" ref="demo_content_2"/>
        <field name="url_path">/2/de</field>
        <field name="language" eval="26"/>
        <field name="modification_date" eval="str(time.localtime(time.time())[0]+1)+'-01-01'"/>
    </record>

    <!-- Versions (Site 2) -->
    <record id="demo_translation_3_fr" model="commown_translation_manager.version">
        <field name="content_path"/>
        <field name="content_id" ref="demo_content_3"/>
        <field name="url_path">/3/fr</field>
        <field name="language" eval="23"/>
        <field name="modification_date" eval="str(time.localtime(time.time())[0]+1)+'-01-01'"/>
    </record>

    <record id="demo_translation_3_de" model="commown_translation_manager.version">
        <field name="content_path"/>
        <field name="content_id" ref="demo_content_3"/>
        <field name="url_path">/3/de</field>
        <field name="language" eval="26"/>
        <field name="modification_date" eval="str(time.localtime(time.time())[0]+1)+'-01-01'"/>
    </record>

    <!-- Requetes (Site 1) -->
    <record id="demo_request_1_fr_to_de" model="commown_translation_manager.translation_request">
        <field name="origin_t10n_id" ref="demo_translation_1_fr"/>
        <field name="target_t10n_id" ref="demo_translation_1_de"/>
        <field name="diffs">Test 1</field>
        <field name="stage_id" ref="stage_new"/>
    </record>

    <record id="demo_request_2_de_to_fr" model="commown_translation_manager.translation_request">
        <field name="origin_t10n_id" ref="demo_translation_2_de"/>
        <field name="target_t10n_id" ref="demo_translation_2_fr"/>
        <field name="diffs">Test 2</field>
        <field name="stage_id" ref="stage_ongoing"/>
    </record>

    <!-- RequÃªtes (Site 2) -->
    <record id="demo_request_3_fr_to_de" model="commown_translation_manager.translation_request">
        <field name="origin_t10n_id" ref="demo_translation_3_fr"/>
        <field name="target_t10n_id" ref="demo_translation_3_de"/>
        <field name="diffs">Test 3</field>
        <field name="stage_id" ref="stage_new"/>
    </record>
</odoo>